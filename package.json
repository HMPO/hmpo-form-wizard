{
  "name": "hmpo-form-wizard",
  "version": "13.0.1",
  "description": "Routing and request handling for a multi-step form processes",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit && npm run test:cover && npm audit --production",
    "test:lint": "eslint .",
    "test:unit": "nyc --reporter=lcov --reporter=text-summary _mocha",
    "test:cover": "nyc check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HMPO/hmpo-form-wizard.git"
  },
  "author": "HMPO",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "bugs": {
    "url": "https://github.com/HMPO/hmpo-form-wizard/issues"
  },
  "homepage": "https://github.com/HMPO/hmpo-form-wizard#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "csrf": "^3.1.0",
    "debug": "^4.3.6",
    "deep-clone-merge": "^1.5.5",
    "hmpo-model": "^5.0.1",
    "moment": "^2.30.1",
    "underscore": "^1.13.7"
  },
  "peerDependencies": {
    "express": ">=4",
    "json5": ">=2"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "eslint": "^8.57.0",
    "express": "^4.19.2",
    "json5": "^2.2.3",
    "mocha": "^10.7.0",
    "nyc": "^17.0.0",
    "proxyquire": "^2.1.3",
    "reqres": "^3.0.1",
    "sinon": "^18.0.0",
    "sinon-chai": "^3.7.0"
  },
  "mocha": {
    "require": "test/helpers",
    "spec": "test/**/spec.*.js"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "bin/**",
      "coverage/**",
      "test/**",
      "example/**"
    ],
    "check-coverage": true,
    "skip-full": true,
    "temp-directory": "coverage/.nyc_output",
    "reporter": [
      "text-summary",
      "text",
      "lcov"
    ],
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
