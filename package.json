  },
  "homepage": "https://github.com/UKHomeOffice/passports-form-wizard",
  "dependencies": {
    "csrf": "^3.0.2",
    "debug": "^2.1.2",
    "express": "^4.12.2",
    "hmpo-form-controller": "^0.11.0",
    "hmpo-model": "^0.7.0",
    "hogan.js": "^3.0.2",
    "i18n-lookup": "^0.1.0",
    "underscore": "^1.8.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
